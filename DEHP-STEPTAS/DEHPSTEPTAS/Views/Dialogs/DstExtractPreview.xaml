<Window
    x:Class="DEHPSTEPTAS.Views.Dialogs.DstExtractPreview"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:DEHPSTEPTAS.Views.Dialogs"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Extraction Preview"
    Width="1658.857"
    Height="767.143"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="720" />
            <RowDefinition Height="32" />

        </Grid.RowDefinitions>
        <TextBox
            Name="myTextBox"
            Grid.Row="0"
            Grid.RowSpan="1"
            Grid.Column="0"
            Height="574"
            Margin="0,18,0,-16.571"
            VerticalAlignment="Top"
            ScrollViewer.CanContentScroll="True"
            ScrollViewer.HorizontalScrollBarVisibility="Auto"
            ScrollViewer.VerticalScrollBarVisibility="Auto"
            Text="{Binding PreviewResult}" />
        <Button
            Grid.Column="0"
            Click="Button_Click"
            Content="Close" Margin="791,654,791.143,27"
            />
    </Grid>
</Window>
